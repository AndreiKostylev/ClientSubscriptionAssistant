<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ClientSubscriptionAssistant.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ClientSubscriptionAssistant"
     xmlns:Views="clr-namespace:ClientSubscriptionAssistant.Views"
    Shell.FlyoutBehavior="Flyout"
    Title="ClientSubscriptionAssistant">

    <FlyoutItem Title="Main">
        <ShellContent
       
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    </FlyoutItem>
    
    <FlyoutItem Title="Login">
        <ShellContent
      
    Title="Login"
    ContentTemplate="{DataTemplate Views:LoginPage}"
    Route="LoginPage" />
        </FlyoutItem>

    <FlyoutItem Title="Register">
    <ShellContent
       
        ContentTemplate="{DataTemplate Views:RegisterPage}"
        Route="RegisterPage" />
    </FlyoutItem>

    <FlyoutItem Title="Subscriptions">
        <ShellContent
    
     ContentTemplate="{DataTemplate Views:SubscriptionPage}"
     Route="SubscriptionPage" />
    </FlyoutItem>
    <FlyoutItem Title="Profile">
        <ShellContent
  
   ContentTemplate="{DataTemplate Views:ProfilePage}"
   Route="ProfilePage" />
    </FlyoutItem>
</Shell>
